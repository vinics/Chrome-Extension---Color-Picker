{
  "name": "color-picker",
  "description": "Chrome extension to get colors from websites and images",
  "version": "0.1.0",
  "manifest_version": 3,
  "permissions": ["activeTab", "downloads", "scripting", "storage"],
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "/imgs/colorPicker-icon-16.png",
    "32": "/imgs/colorPicker-icon-32.png",
    "48": "/imgs/colorPicker-icon-48.png",
    "128": "/imgs/colorPicker-icon-128.png"
  }
}
